						<div class="card">
							<div class="card-header">
								<h4 class="card-title d-inline-block">Upcoming Donors number <%= appointment%></h4> <a href="/donors" class="btn btn-primary float-right">View all</a>
							</div>
							<div class="card-body p-0">
								<div class="table-responsive">
									<table class="table mb-0">
										<thead class="d-none">
											<tr>
												<th>Patient Name</th>
												<th>Doctor Name</th>
												<th>Timing</th>
												
											</tr>
										</thead>
										<tbody>
                                            <% for (var i = 0; i < applist.length; i++){ %>
											<tr>
												<td style="min-width: 200px;">
													<h5 class="time-title p-0">Patient Name</h5>
													<p><%= applist[i].patient_name%></p>
												</td>                 
												<td>
													<h5 class="time-title p-0">Appointment With</h5>
													<p><%= applist[i].doctor_name%></p>
												</td>
												<td>
													<h5 class="time-title p-0">Timing</h5>
													<p><%= applist[i].time%></p>
												</td>
												<td class="text-right">
													<a href="/donors" class="btn btn-outline-primary take-btn">Take up</a> 
                                                </td>
                                                <% } %>
											</tr>
											
											
											
										
										</tbody>
									</table>
								</div>
							</div>
						</div>
					</div>
